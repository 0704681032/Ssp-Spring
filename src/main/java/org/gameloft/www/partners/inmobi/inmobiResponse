package org.gameloft.www.partners.inmobi.model;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonProperty;
import com.fasterxml.jackson.annotation.JsonPropertyOrder;

import java.util.ArrayList;

/**
 * Created by ningzhen.ge on 2016/9/29.
 */
@JsonInclude(JsonInclude.Include.NON_NULL)
@JsonPropertyOrder({"ads","requestId"})
public class InmobiResponse {

    @JsonProperty
    private ArrayList<InmobiAd> ads = new ArrayList<InmobiAd>();

    @JsonProperty
    private String requestId;

    @JsonProperty
    public ArrayList<InmobiAd> getAds() {
        return ads;
    }

    @JsonProperty
    public void setAds(ArrayList<InmobiAd> ads) {
        this.ads = ads;
    }

    @JsonProperty
    public String getRequesId() {
        return requestId;
    }

    @JsonProperty
    public void setRequesId(String requestId) {
        this.requestId = requestId;
    }
}
