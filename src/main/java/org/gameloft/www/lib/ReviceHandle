package org.gameloft.www.lib.Review;

import org.gameloft.www.lib.Logger.LogService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import javax.annotation.PostConstruct;

/**
 * Created by ningzhen.ge on 2016/10/24.
 */
@Component
public class ReviewHandle {

    /**
     * Spark endpoint
     */
    private static final String endPoint = "http://198.136.47.52:900/save";

    private String content;

    private String creativeId;

    private String dsp;

    private String flag;

    private String partner;

    /**
     * The Redis key  format used to store blacklist creatives per partner name
     */
    private static final String redisBlacklistAdKeyFormat = "blacklistAds:%s";

    /**
     * The Redis key formate uses to store whitelist creatives perpartner name for licensed game
     */
    private static final String redisWhitelistLicensedAdKeyFormat = "whitelistLicensedAds:%s";

    /**
     * logger
     */
    @Autowired
    private LogService logService;

    @PostConstruct
    public void init()
    {

    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getCreativeId() {
        return creativeId;
    }

    public void setCreativeId(String creativeId) {
        this.creativeId = creativeId;
    }

    public String getDsp() {
        return dsp;
    }

    public void setDsp(String dsp) {
        this.dsp = dsp;
    }

    public String getFlag() {
        return flag;
    }

    public void setFlag(String flag) {
        this.flag = flag;
    }

    public String getPartner() {
        return partner;
    }

    public void setPartner(String partner) {
        this.partner = partner;
    }
}
